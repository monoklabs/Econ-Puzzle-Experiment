{
  "name": "frontendPapaya",
  "description": "Papaya - frontend",
  "repository": "",
  "dependencies": {
    "leaflet": "^0.7.7",
    "moment": "^2.29.2",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-collapse": "^2.3.3",
    "react-countdown-clock": "^2.4.0",
    "react-dom": "^15.3.2",
    "react-height": "^2.1.1",
    "react-motion": "^0.4.5",
    "react-numeric-input": "^2.0.7",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-svg-gauge": "^1.0.8",
    "react-timeago": "^3.1.3",
    "react-webcam": "^1.0.1",
    "redux": "^3.6.0",
    "request": "^2.76.0",
    "socket.io-client": "^1.7.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^13.1.1",
    "browserify-incremental": "^3.1.1",
    "catw": "^1.0.1",
    "clean-css": "^3.4.20",
    "compression": "^1.6.2",
    "cron": "^1.1.1",
    "envify": "^3.4.1",
    "express": "^4.14.0",
    "minimist": "^1.1.1",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "serve": "^1.4.0",
    "uglify-js": "^2.7.4",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "start": "npm run build-dev & npm run watch-js & npm run watch-css & npm run watch-html & npm run serve-dev",
    "serve": "NODE_ENV=production node server.js -p 80 ../public",
    "serve-dev": "NODE_ENV=development node server.js -p 3001 ../public",
    "build": "npm run print-starting && npm run build-js-min && npm run build-css && npm run move-html",
    "build-dev": "npm run build-js && npm run build-css && npm run move-html",
    "build-js": "npm run print-starting && LAST_COMMIT=`git log -n 1` NODE_ENV=development browserifyinc -v -t [babelify] -t envify -e src/js/app.jsx -o ../public/app.js --cachefile .browserify-cache.json && npm run print-finished",
    "build-js-min": "npm run print-starting && LAST_COMMIT=`git log -n 1` NODE_ENV=production browserifyinc -v -t [babelify] -t envify -e src/js/app.jsx -o ../public/app.js --cachefile .browserify-cache.json && uglifyjs ../public/app.js --compress --mangle -o ../public/app.js && npm run print-finished",
    "build-css": "cat src/css/*.css src/css/**/*.css | cleancss -o ../public/app.css",
    "move-html": "cat src/index.html > ../public/index.html",
    "clean": "rm ../public/app.js && rm ../public/app.css && rm ../public/index.html",
    "print-starting": "DATE=`date '+%Y-%m-%d %H:%M:%S'`;NC='\\033[0;0m';BROWN='\\033[0;33m';echo \"${BROWN}${DATE} Starting react build..${NC}\";",
    "print-finished": "DATE=`date '+%Y-%m-%d %H:%M:%S'`;NC='\\033[0;0m';BROWN='\\033[0;33m';echo \"${BROWN}${DATE} Finished building react app..${NC}\";",
    "watch-html": "catw -w src/index.html -o ../public/index.html",
    "watch-js-old": "NODE_ENV=development watchify -t [babelify] -t envify src/js/app.jsx -o ../public/app.js",
    "watch-js": "nodemon --watch ./src/js -e js,jsx --exec 'npm run build-js'",
    "watch-css": "catw -w src/css/*.css src/css/**/*.css -o ../public/app.css"
  }
}
